{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": ["warn", {"vars": "all", "args": "none", "varsIgnorePattern": "[cC]lient|[cC]ommand|[cC]ollection|[mM]essage|[gG]uild|DMChannel|^_"}]
    }
}
